{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "skipLibCheck": true,
    "allowJs": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "moduleResolution": "node",
    "rootDir": "./test/webapp",
    "outDir": "./test/dist",
    "baseUrl": "./test",
    "paths": {
      "*": [
        "./test/webapp/*"
      ]
    },
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/@sapui5/types"
    ]
  },
  "include": [
    "./test/webapp/**/*"
  ]
}
